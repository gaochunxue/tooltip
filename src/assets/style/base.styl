px($val) {
  1rem * ($val / 20)
}
ellipsis($val) {
  text-overflow: -o-ellipsis-lastline
  overflow: hidden
  text-overflow: ellipsis
  display: -webkit-box
  -webkit-line-clamp: $val
  -webkit-box-orient: vertical
}
clearfix() {
  .clearfix {
    *zoom: 1
  }
  .clearfix:after {
    content: ''
    display: block
    line-height: 0
    height: 0
    visibility: hidden
    clear: both
  }
}
